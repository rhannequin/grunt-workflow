{
  "localhost": {
    "hostname": "http://127.0.0.1",
    "port": 3000
  },
  "bower": "bower_components",
  "local": {
    "_": "app",
    "assets": {
      "_": "<%= config.local._ %>/assets",
      "images": "<%= config.local.assets._ %>/images",
      "scripts": "<%= config.local.assets._ %>/scripts",
