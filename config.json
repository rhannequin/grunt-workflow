      "images": "<%= config.dist.assets._ %>/images",
      "scripts": "<%= config.dist.assets._ %>/scripts",
      "styles": "<%= config.dist.assets._ %>/styles",
      "sass": "<%= config.dist.assets.styles %>/sass",
      "fonts": "<%= config.dist.assets._ %>/fonts"
    }
